{
    "name": "macellan/ileti-merkezi",
    "description": "Ileti Merkezi SMS notification channel for Laravel",
    "homepage": "http://macellan.net/",
    "license": "MIT",
    "authors": [
        {
            "name": "Fatih Aytekin",
            "email": "fatih@aytekin.me",
            "homepage": "https://github.com/faytekin",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^8.0|^10.0",
        "illuminate/http": "^7.0|^8.0|^9.0|^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Macellan\\IletiMerkezi\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Macellan\\IletiMerkezi\\IletiMerkeziServiceProvider"
            ]
        }
    }
}
